package com.acme.app.dto.device;

import java.io.Serializable;
import java.util.List;

/**
 * 站点的历史数据
 * @author YL.Z
 *
 * @date 2017年4月12日 下午2:47:53
 */
public class SiteDataChart implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 3179733771350460483L;
	
	private List<Integer> historyScore;//历史评分
	private List<Double> codmn;
	private List<Double> ph;
	private List<Double> wdo;
	private List<Double> nhthn;
	
	public SiteDataChart(){
		
	}
	public SiteDataChart(List<Double> codmn, List<Double> ph, List<Double> wdo, List<Double> nhthn) {
		super();
		this.codmn = codmn;
		this.ph = ph;
		this.wdo = wdo;
		this.nhthn = nhthn;
	}
	
	public List<Integer> getHistoryScore() {
		return historyScore;
	}
	public void setHistoryScore(List<Integer> historyScore) {
		this.historyScore = historyScore;
	}
	public List<Double> getCodmn() {
		return codmn;
	}
	public void setCodmn(List<Double> codmn) {
		this.codmn = codmn;
	}
	public List<Double> getPh() {
		return ph;
	}
	public void setPh(List<Double> ph) {
		this.ph = ph;
	}
	public List<Double> getWdo() {
		return wdo;
	}
	public void setWdo(List<Double> wdo) {
		this.wdo = wdo;
	}
	public List<Double> getNhthn() {
		return nhthn;
	}
	public void setNhthn(List<Double> nhthn) {
		this.nhthn = nhthn;
	}
	
	
}
